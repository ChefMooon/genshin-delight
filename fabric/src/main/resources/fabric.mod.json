{
  "schemaVersion": 1,
  "id": "${mod_id}",
  "version": "${mod_version}",
  "name": "${mod_name}",
  "description": "${mod_description}",
  "authors": [
    "${mod_authors}"
  ],
  "contact": {
    "homepage": "${mod_homepage}",
    "issues": "${mod_issues}",
    "sources": "${mod_github}"
  },
  "license": "${mod_license}",
  "icon": "",
  "environment": "*",
  "entrypoints": {
    "main": [
      "com.chefmooon.genshindelight.fabric.ModFabric"
    ],
    "client": [
      "com.chefmooon.genshindelight.client.fabric.ModClientImpl"
    ],
    "fabric-datagen": [
      "com.chefmooon.genshindelight.data.fabric.DataGeneration"
    ]
  },
  "mixins": [
    "${mod_id}.mixins.json",
    "${mod_id}-common.mixins.json"
  ],
  "depends": {
    "fabric": "*",
    "minecraft": ">=${minecraft_min_version} <=${minecraft_max_version}",
    "farmersdelight": "*"
  },
  "custom": {
    "modmenu": {
      "links": {
        "modmenu.wiki": "${mod_homepage}",
        "modmenu.discord": "${mod_discord}"
      }
    }
  }
}